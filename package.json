{
  "name": "trendyol-profit-calculator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:test": "VITE_TRENDYOL_ENV=test vite --host 0.0.0.0 --port 5173",
    "build": "vite build",
    "build:test": "VITE_TRENDYOL_ENV=test vite build --mode test",
    "lint": "eslint .",
    "preview": "vite preview",
    "server:dev": "cd server && npm run dev",
    "server:test": "cd server && TRENDYOL_ENV=test node trendyol-proxy.js",
    "server:start": "cd server && npm start",
    "server:install": "cd server && npm install",
    "build:full": "npm run build && npm run server:install",
    "build:production": "NODE_ENV=production vite build --mode production",
    "test:start": "concurrently \"npm run server:test\" \"npm run dev:test\""
  },
  "dependencies": {
    "firebase": "^10.13.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.2",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.26.1",
    "recharts": "^3.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "terser": "^5.43.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
