{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/trendyol-proxy.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "TRENDYOL_ENV": "test"
  },
  "functions": {
    "server/trendyol-proxy.js": {
      "maxDuration": 10
    }
  }
}